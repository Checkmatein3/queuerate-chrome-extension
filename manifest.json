{
    "name": "Queuerate",
    "version": "1.0",
    "permissions": [
            "activeTab", 
            "declarativeContent", 
            "storage", 
            "identity",
            "tabs",
            "cookies",
            "https://www.googleapis.com/*"], 
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
    "description": "Article categorization service that applies any matching user-defined categories to any newly saved articles",
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
      },
    "browser_action": {
        "default_icon": {
          "16": "images/get_started16.png",
          "32": "images/get_started32.png",
          "48": "images/get_started48.png",
          "128": "images/get_started128.png"
        }
    },
    "manifest_version": 2
  }